   "source": [
    "import requests\n",
    "\n",
    "def fetchAndSaveToFile(url, path):\n",
    "    r = requests.get(url)\n",
    "    with open(path, \"wb\") as f:  # Use 'wb' mode instead of 'w'\n",
    "        f.write(r.content)  # Use r.content to write binary data\n",
    "\n",
    "url = 'https://www.dawn.com/'\n",
    "fetchAndSaveToFile(url, \"new_dawn.html\")\n"
   ]
  }
